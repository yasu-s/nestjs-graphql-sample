{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nestjs-graphql-sample/src",
  "monorepo": true,
  "root": "apps/nestjs-graphql-sample",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/nestjs-graphql-sample/tsconfig.app.json"
  },
  "projects": {
    "nestjs-graphql-sample": {
      "type": "application",
      "root": "apps/nestjs-graphql-sample",
