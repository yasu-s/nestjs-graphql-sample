      "sourceRoot": "apps/nestjs-graphql-sample/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nestjs-graphql-sample/tsconfig.app.json"
      }
    },
    "schema-first": {
      "type": "application",
      "root": "apps/schema-first",
      "entryFile": "main",
      "sourceRoot": "apps/schema-first/src",
      "compilerOptions": {
        "tsConfigPath": "apps/schema-first/tsconfig.app.json"
      }
    }
  }
}